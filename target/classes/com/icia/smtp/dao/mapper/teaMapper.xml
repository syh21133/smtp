<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="MovieTeaser">
	
	<!-- MOV010 : 영화 정보 상세보기(포스터 정보) -->
	<select id="teaView" parameterType="String" resultType="movieteaser">
		SELECT * FROM MOVIE_TEASER WHERE TEATITLE = #{mvTitle}
	</select>
	
	
	
</mapper>